<html>
<head>
<link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
<link href="static/css/myCss.css" rel="stylesheet">
<meta charset="UTF-8">
<link href="static/css/bootstrap.min.css" rel="stylesheet" media="screen">
<link href="static/css/customIndex.css" rel="stylesheet">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link href="static/css/bootstrap-responsive.css" rel="stylesheet">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">

<title>Index</title>
</head>
<body>
  <form action="" method="post">
  <div class="container">
    <h2>BREAD 2 (Browse, Read, Edit, Add, Delete)</h2>
    <h4>Filters</h4>
    
    <div class="form-group row">
      <label for="id" class="col-sm-2 col-form-label">
        <input type="checkbox" name="cekid" id="id" value="valid">ID</input></label>
        <div class="col-sm-10">
          <input type="email" class="form-control" id="id" placeholder="ID">
        </div>
    </div>
    
    <div class="form-group row">
      <label for="string" class="col-sm-2 col-form-label">
        <input type="checkbox" name="cekstring" id="str" value="string">String</input></label>
        <div class="col-sm-10">
          <input type="email" class="form-control" id="string" placeholder="String">
        </div>
    </div>

    <div class="form-group row">
      <label for="integer" class="col-sm-2 col-form-label">
        <input type="checkbox" name="cekint" id="int" value="integer">Integer</input></label>
        <div class="col-sm-10">
          <input type="email" class="form-control" id="integer" placeholder="Integer">
        </div>
    </div>

    <div class="form-group row">
      <label for="float" class="col-sm-2 col-form-label">
        <input type="checkbox" name="cekflo" id="flt" value="float">Float</input></label>
        <div class="col-sm-10">
          <input type="email" class="form-control" id="float" placeholder="Float">
        </div>
    </div>
    
    <div class="form-group row">
      <label for="date" class="col-sm-2 col-form-label">
      <input type="checkbox" name="cekdate" id="dt" value="date">Date</input></label>
      <div class="col-xs-2">
        <input class="form-control" id="startdate" type="text" placeholder="start date">
        <label for="date">to</label>
        <input class="form-control" id="enddate" type="text" placeholder="end date">
      </div>
    </div>

    <div class="form-group row">
      <label for="float" class="col-sm-2 col-form-label">
        <input type="checkbox" name="cekbool" id="bln" value="boolean">Boolean</input></label>
      <div class="col-sm-10">
        <label for="bool"></label>
      <select class="form-control" id="bool">
        <option>true</option>
        <option>false</option>
      </select>
      </div>

    </div>
    <div class="form-group row">
      <button type="submit" class="btn btn-primary">Search</button>
    </div>
      
    </form>
  </div>

  <div class="container">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>ID</th>
          <th>String</th>
          <th>Integer</th>
          <th>Float</th>
          <th>Date</th>
          <th>Boolean</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        
        <%for (const br of model) {%>
          <tr>
          <td><%=br.id%></td>
          <th><%=br.string%></th>
          <th><%=br.integer%></th>
          <th><%=br.float%></th>
          <th><%=br.date%></th>
          <td><%=br.boolean%></td>
        <td>
          <a type="button" class="btn btn-success" href="/editt/<%=br.id%>">Edit</a>
          
           <a type="button" class="btn btn-danger" href="/submitdel/<%=br.id%>">Delete</a></td>
        </tr>
        <%}%>
        
      </tbody>
    </table>
  </div>
  <div class="container">
    <nav aria-label="Page navigation">
      <ul class="pagination">
        <li>
          <a href="#" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
          </a>
        </li>
        <li><a href="#">1</a></li>
        <li><a href="#">2</a></li>
        <li><a href="#">3</a></li>
        <li><a href="#">4</a></li>
        <li><a href="#">5</a></li>
        <li>
          <a href="#" aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
          </a>
        </li>
      </ul>
    </nav>
  </div>

  <div class="container">

    <button type="button" class="btn btn-primary"><a href="/add">Add</a></button>

  </div>

</form>
  


    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="js/bootstrap.min.js"></script>

    
  </body>
</html>